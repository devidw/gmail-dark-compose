{
  "manifest_version": 2,
  "name": "Gmail dark compose",
  "description": "Dark New Message Area in Gmail",
  "version": "0.1",
  "icons": {
    "32": "./icons/32.png"
  },
  "browser_action": {
    "icon": ["./icons/32.png"]
  },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "css": [
      "./css/styles.min.css"
    ]
  }]
}
